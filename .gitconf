[alias]
  l = log --pretty=format:\"%C(yellow)%h%C(reset) | %s %C(red)[%aN]%C(green)%d%C(reset)\" --decorate --graph
  co = checkout
  cm = commit
  ci = commit
  st = status
  br = branch
  mr = merge
[core]
  excludesfile = ~/.gitignore
[user]
  name = Programmer
  email = programmer@example.com
[push]
  default = simple